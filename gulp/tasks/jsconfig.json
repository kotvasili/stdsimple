{
	"extends": "eslint:recommended",
	"rules": {
		"no-alert": 0,
		"no-console": 0,
		"no-bitwise": 0,
		"no-trailing-spaces": 1,
		"no-multi-spaces": 1,
		"no-mixed-spaces-and-tabs": 1,
		"camelcase": 1,
		"eqeqeq": 0,
		"guard-for-in": 1,
		"indent": ["error", "tab"],
		"no-empty": 1
	},
	"globals": {
		"$": true,
		"jQuery": true
	},
	"env": {
		"browser": true,
		"node": true
	}
}