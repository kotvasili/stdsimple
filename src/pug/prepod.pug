		.row
			.col.s6
				.profile-card-wrapper.row
					.profile-card.z-depth-2.col.s4
						i.large.material-icons account_circle
					.profile-card-text.col.s8
						p.bold Шведова О.А
			.col.s6
				a(href="#", data-position="bottom", data-tooltip="Архив групп с прошедших практик").btn.tooltipped Архив
		.groups-wrapper
			.groups-wrapper-nav
			// .loading вешаем во время аякса
			.groups-wrapper-content
				//block for loading
				.block-placeholder
					.preloader-wrapper.big.active
						.spinner-layer.spinner-blue
							.circle-clipper.left
								.circle
							.gap-patch
								.circle
							.circle-clipper.right
								.circle
						.spinner-layer.spinner-red
							.circle-clipper.left
								.circle
							.gap-patch
								.circle
							.circle-clipper.right
								.circle
						.spinner-layer.spinner-yellow
							.circle-clipper.left
								.circle
							.gap-patch
								.circle
							.circle-clipper.right
								.circle
						.spinner-layer.spinner-green
							.circle-clipper.left
								.circle
							.gap-patch
								.circle
							.circle-clipper.right
								.circle
				ul.collapsible.tooltipped(data-collapsible='collapsible', data-position="top", data-tooltip="Нажмите, чтобы раскрыть список")
					li.spec-wrapper
						.collapsible-header.waves-effect.waves-orange
							h5 Группы специальности: ИТУТС
						.collapsible-body
							ul.collapsible(data-collapsible='expandable')
