									.collapsible-body
										.groups-item-contact
											.groups-item-contact-block
												.groups-item-contact-name 
													b Староста
												.groups-item-contact-content 
													p Солодарь В.А.
													p 
														a(href="mailto:kot_vasili@mail.ru") kot_vasili@mail.ru
